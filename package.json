  "repository": {
    "type": "git",
    "url": "https://github.com/mbifulco/blog"
  },
  "bugs": {
    "url": "https://github.com/mbifulco/blog/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,md}": [
      "prettier --write",
      "git add"
